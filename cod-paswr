
	public static String codigo;
	public static String descripcion;
	public static String tipo[];
	public static int cantidad;
	public static double costounitario;
	public static double costomantenimiento;
	public static double costotraslado;
	public static String getCodigo() {
		return codigo;
	}
	public static void setCodigo(String codigo) {
		Hospital.codigo = codigo;
	}
	public static String getDescripcion() {
		return descripcion;
	}
	public static void setDescripcion(String descripcion) {
		Hospital.descripcion = descripcion;
	}
	public static String[] getTipo() {
		return tipo;
	}
	public static void setTipo(String[] tipo) {
		Hospital.tipo = tipo;
	}
	public static int getCantidad() {
		return cantidad;
	}
	public static void setCantidad(int cantidad) {
		Hospital.cantidad = cantidad;
	}
	public static double getCostounitario() {
		return costounitario;
	}
	public static void setCostounitario(double costounitario) {
		Hospital.costounitario = costounitario;
	}
	public static double getCostomantenimiento() {
		return costomantenimiento;
	}
	public static void setCostomantenimiento(double costomantenimiento) {
		Hospital.costomantenimiento = costomantenimiento;
	}
	public static double getCostotraslado() {
		return costotraslado;
	}
	public static void setCostotraslado(double costotraslado) {
		Hospital.costotraslado = costotraslado;
	}
	
